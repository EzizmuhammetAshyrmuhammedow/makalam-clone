		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^3.6.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"unocss": "^0.62.3",
		"vite": "^5.0.3",
		"vitest": "^2.0.0"
	},
	"type": "module",
	"dependencies": {
		"@mdi/font": "^7.4.47",
		"@supabase/supabase-js": "^2.45.2",
		"uno-preset-dark-theme": "^0.0.6"
	}
